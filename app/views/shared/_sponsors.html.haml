%section.separator#sponsors--anchor
  .container
    %h1.block-title
      Our Sponsors

%section.separator
  .container.supporters__block
    - sponsors.each do |sponsor|
      .supporters__block--item
        = link_to sponsor.website, target: :_blank, class: "" do
          - if sponsor&.image_url
            = image_tag sponsor&.image_url
          - else
            = image_tag("london-girl.png")
