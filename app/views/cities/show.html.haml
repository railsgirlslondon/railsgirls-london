- if @city.slug.eql? "london"
  =render partial: 'subscribe'

=render partial: 'upcoming_event', locals: { event: @city.upcoming_event } unless @city.upcoming_event.nil?

- if allow_feedback?(@latest_event)
  =render partial: 'shared/ask_for_feedback', locals: { event: @latest_event, city: @city }

=render partial: 'upcoming_meetings', locals: { meetings: @city.meetings.upcoming } unless @city.meetings.upcoming.nil?

=render partial: "past_events", locals: { events: @city.past_events } unless @city.past_events.empty?

=render partial: 'shared/all_sponsors', locals: { sponsors: @city.sponsors, show_title: false }

