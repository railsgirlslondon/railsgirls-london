= simple_form_for([:admin, @city, @member], html: {class: "form-horizontal"}) do |f|
  = f.error_notification

  .form-inputs
    = f.input :first_name
    = f.input :last_name
    = f.input :email
    = f.input :phone_number
    = f.input :twitter

  .form-actions
    = f.button :submit, class: 'btn btn-large'
